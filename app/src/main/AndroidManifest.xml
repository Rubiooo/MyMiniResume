<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rubioo.myminiresume">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".BasicInfoEditActivity"
            android:label="@string/title_activity_basic_info_edit"
             />
        <activity
            android:name=".EducationEditActivity"
            android:label="@string/title_activity_education_edit"
             />
        <activity
            android:name=".ExperienceEditActivity"
            android:label="@string/title_activity_experience_edit"
             />
        <activity
            android:name=".ProjectEditActivity"
            android:label="@string/title_activity_project_edit"
            />
    </application>

</manifest>